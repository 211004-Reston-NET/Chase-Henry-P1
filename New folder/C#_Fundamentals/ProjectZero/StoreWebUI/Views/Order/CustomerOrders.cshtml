@{
}

@model IEnumerable<StoreWebUI.Models.OrderVM>

<h1> List of Customer Orders By ID</h1>

<br />

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>Order ID</th>
            <th>Total</th>
            <th>Customer ID</th>
            <th>Store ID</th>
            <th>Product ID</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var ord in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => ord.OrderId)</td>
                <td>@Html.DisplayFor(modelItem => ord.Total)</td>
                <td>@Html.DisplayFor(modelItem => ord.CustId)</td>
                <td>@Html.DisplayFor(modelItem => ord.StoreId)</td>
                <td>@Html.DisplayFor(modelItem => ord.prodId)</td>
            </tr>
        }
    </tbody>
</table>